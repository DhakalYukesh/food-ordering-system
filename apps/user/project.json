{
  "name": "user",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/user/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "set NODE_ENV=production && node ../../node_modules/webpack-cli/bin/cli.js build",
        "cwd": "apps/user"
      },
      "configurations": {
        "development": {
          "command": "set NODE_ENV=development && node ../../node_modules/webpack-cli/bin/cli.js build",
          "cwd": "apps/user"
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "user:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "user:build:development"
        },
        "production": {
          "buildTarget": "user:build:production"
        }
      }
    }
  }
}
